<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Spring Boot 2.5.7 - Versión estable para este proyecto -->
    <suppress>
        <notes>Spring Boot 2.5.7 es una versión LTS para compatibilidad con Spring Cloud 2020.0.4</notes>
        <cve>CVE-2023-20860</cve>
        <cve>CVE-2023-20861</cve>
    </suppress>
    
    <!-- H2 Database - Solo usado en desarrollo/testing -->
    <suppress>
        <notes>H2 Database solo se usa en entorno de desarrollo y testing, no en producción</notes>
        <gav regex="true">^com\.h2database:h2:.*$</gav>
    </suppress>
</suppressions>
